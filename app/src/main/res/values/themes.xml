<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NotificationDictionary" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2196f3</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#2196f3</item>
        <!-- Customize your theme here. -->
        <item name="lexical_category_background_color">#CFADAD</item>
        <item name="lexical_category_text_color">#232F34</item>
        <item name="meaning_card_background_color">#DDCECE</item>
        <item name="meaning_text_color">#232F34</item>
    </style>

    <style name="Theme.NotificationDictionary.Dark" parent="ThemeOverlay.MaterialComponents.Dark">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#1C1919</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#1F2C37</item>
        <!-- Customize your theme here. -->
        <item name="lexical_category_background_color">#332C2C</item>
        <item name="lexical_category_text_color">#FFFFFF</item>
        <item name="meaning_card_background_color">#1C1919</item>
        <item name="meaning_text_color">#FFFFFF</item>
    </style>
</resources>